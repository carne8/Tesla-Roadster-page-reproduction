{
  "name": "fsharp-web-template",
  "version": "1.0.0",
  "description": "A template to create web applications with F# and React",
  "license": "MIT",
  "private": true,
  "scripts": {
    "preinstall": "dotnet restore && dotnet tool restore && paket install",
    "start": "dotnet fable watch src --define DEBUG --run webpack serve",
    "build": "dotnet fable src && webpack"
  },
  "dependencies": {
    "node-polyglot": "^2.4.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "css-loader": "^6.5.1",
    "dotenv-webpack": "^7.0.3",
    "html-webpack-plugin": "^5.5.0",
    "remotedev": "^0.2.9",
    "sass": "^1.46.0",
    "sass-loader": "^12.4.0",
    "style-loader": "^3.3.1",
    "webp-loader": "^0.6.0",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.2"
  }
}
